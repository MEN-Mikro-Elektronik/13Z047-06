<?xml version="1.0" encoding="UTF-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="packagedesc.xsd">
	<name>13z04706</name>
	<description>MDIS5 driver package for MEN 16Z047 Watchdog</description>
	<date>2016-07-11</date>
	<revision>0.1</revision>
	<docroot>DRIVERS/MDIS_LL/Z047/DOC/html/index.html</docroot>
	<!-- Models -->
	<modellist>
		<model>
			<!-- standard model -->
			<hwname>16Z047_WDOG</hwname>
			<description>Watchdog in Chameleon FPGA</description>
			<devnameprefix>WDOG</devnameprefix>
			<devtype>MDIS</devtype>
            <autoid>
                <chamv2id>47</chamv2id>
            </autoid>
            <bbslot>
                <bbismodel>CHAMELEON</bbismodel>
            </bbslot>
            <bbslot>
                <bbismodel>CHAMELEON_PCITBL</bbismodel>
            </bbslot>
