	</modellist>
	<!-- Global software modules -->
	<swmodulelist>
		<swmodule>
			<name>z47</name>
			<description>Watchdog Driver</description>
			<type>Low Level Driver</type>
			<makefilepath>Z047/DRIVER/COM/driver.mak</makefilepath>
		</swmodule>
		<swmodule>
			<name>wdog_test</name>
			<description>Test program for WDOG profile drivers drivers</description>
			<type>Common Tool</type>
			<makefilepath>WDOG/WDOG_TEST/COM/program.mak</makefilepath>
		</swmodule>
		<swmodule>
			<name>wdog_simp</name>
			<description>Simple example program for WDOG profile drivers drivers</description>
			<type>Common Tool</type>
			<makefilepath>WDOG/WDOG_SIMP/COM/program.mak</makefilepath>
		</swmodule>
		<swmodule>
			<name>wdog_ctrl</name>
			<description>Control tool for WDOG profile drivers</description>
			<type>Common Tool</type>
			<makefilepath>WDOG/WDOG_CTRL/COM/program.mak</makefilepath>
		</swmodule>
	</swmodulelist>
